Algoritmo distancia
	Escribir sin saltar "Ingresa el valor de distancia en metros"
	Leer distancia_en_metros
	Escribir sin saltar "ingresa el valor de tiempo en minutos"
	Leer tiempo_en_minutos;
	velocidad_en_km_por_h<-distancia_en_metros/1000/tiempo_en_minutos/60
	Escribir "valor de velocidad_en_kn_por_h;" velocidad_en_km_por_h


	
	
	
FinAlgoritmo
